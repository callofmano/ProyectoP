<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="290.0" prefWidth="431.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.poo2.CreardController">
    <children>
        <Label text="Agregar Dueño">
         <font>
            <Font name="Arial" size="20.0" />
         </font></Label>
      <HBox alignment="CENTER" prefHeight="150.0" prefWidth="225.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label layoutX="10.0" layoutY="10.0" prefHeight="25.0" prefWidth="102.0" text="Nombres:" />
                  <Label layoutX="10.0" layoutY="27.0" prefHeight="25.0" prefWidth="101.0" text="Apellidos:" />
                  <Label layoutX="10.0" layoutY="27.0" prefHeight="27.0" prefWidth="101.0" text="Dirección:" />
                  <Label layoutX="10.0" layoutY="27.0" prefHeight="23.0" prefWidth="101.0" text="Teléfono:" />
                  <Label layoutX="10.0" layoutY="10.0" prefHeight="27.0" prefWidth="100.0" text="Ciudad:" />
                  <Label layoutX="10.0" layoutY="10.0" prefHeight="24.0" prefWidth="104.0" text="Ci:" />
                  <Label text="E-mail" />
               </children>
               <opaqueInsets>
                  <Insets right="20.0" />
               </opaqueInsets>
            </VBox>
            <VBox prefHeight="150.0" prefWidth="164.0">
               <children>
                  <TextField fx:id="txtNombre" prefHeight="25.0" prefWidth="132.0" />
                  <TextField fx:id="txtApellidos" layoutX="10.0" layoutY="10.0" />
                  <TextField layoutX="10.0" layoutY="35.0" />
                  <TextField fx:id="txtTelefono" layoutX="10.0" layoutY="10.0" />
                  <ComboBox fx:id="comboCiudad" prefHeight="25.0" prefWidth="164.0" />
                  <TextField fx:id="txtCi" layoutX="10.0" layoutY="10.0" />
                  <TextField fx:id="txtEmail" />
               </children>
               <opaqueInsets>
                  <Insets left="50.0" />
               </opaqueInsets>
            </VBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="33.0" prefWidth="285.0">
         <children>
            <Button fx:id="botonGuardar" alignment="TOP_LEFT" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="25.0" prefWidth="65.0" text="Guardar">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </Button>
              <Button fx:id="secondaryButton" alignment="CENTER" contentDisplay="CENTER" onAction="#switchToAdministrarDuenos" prefHeight="25.0" prefWidth="70.0" text="Cancelar">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Button fx:id="botonRegresar" mnemonicParsing="false" onAction="#switchToMenuPrincipal" text="Regresar Menú" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="50.0" top="20.0" />
    </padding>
</VBox>
