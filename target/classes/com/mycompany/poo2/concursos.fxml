<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="382.0" prefWidth="692.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.poo2.ConcursosController">
   <children>
      <Label alignment="TOP_CENTER" contentDisplay="TOP" prefHeight="0.0" prefWidth="266.0" text="Administrar Concursos" textAlignment="RIGHT">
         <font>
            <Font size="25.0" />
         </font></Label>
      <TableView fx:id="listaConcursos" prefHeight="227.0" prefWidth="514.0">
        <columns>
          <TableColumn fx:id="colCodigo" prefWidth="75.0" text="Código" />
          <TableColumn fx:id="colNombre" prefWidth="104.79993438720703" text="Nombre" />
            <TableColumn fx:id="colFecha" prefWidth="74.39996337890625" text="Fecha" />
            <TableColumn fx:id="colCiudad" prefWidth="76.800048828125" text="Ciudad" />
            <TableColumn fx:id="colOpciones" prefWidth="320.7999572753906" text="Opciones" />
        </columns>
      </TableView>
      <HBox alignment="CENTER" prefHeight="52.0" prefWidth="494.0">
         <children>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#switchToCrearConcursos" prefHeight="26.0" prefWidth="122.0" text="Crear Concurso" translateX="-30.0" />
            <Button fx:id="botonEnviarInvitaciones" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#enviarInvitaciones" prefHeight="26.0" prefWidth="124.0" text="Enviar invitaciones" textAlignment="CENTER" translateX="30.0" />
         </children>
      </HBox>
      <Button mnemonicParsing="false" onAction="#switchToMenuPrincipal" text="Regresar Menú Principal">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
